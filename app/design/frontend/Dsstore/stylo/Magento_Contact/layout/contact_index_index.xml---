<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Contact Us</title>
        <link rel="canonical" src="https://stores.dotsquares.com/contact" src_type="url" />
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Dotsquares\Stylo\Block\Template" name="google_map" template="Magento_Contact::google_map.phtml" before="-"/>
            <block class="Dotsquares\Stylo\Block\Template" name="contactPage" template="Magento_Contact::contact_page.phtml">
                <block class="Magento\Contact\Block\ContactForm" name="contactForm" template="Magento_Contact::form.phtml">
                    <container name="form.additional.info" label="Form Additional Info"/>
                </block>
            </block>
        </referenceContainer>
    </body>
</page>
