<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<body>
    <referenceBlock name="customer_account_navigation">
        <block class="Magento\Framework\View\Element\Html\Link\Current"  ifconfig="cusotmerdelete/gdprlaw/gdpr_enable" name="customer-account-navigation-gdpr-index-gdpr" after="-">
            <arguments>
                <argument name="path" xsi:type="string">gdpr/index/gdpr</argument>
                <argument name="label" xsi:type="string">GDPR Policy</argument>
            </arguments>
        </block>
        <block class="Magento\Framework\View\Element\Html\Link\Current"  ifconfig="cusotmerdelete/general/deletecusotmer_enable" name="customer-account-navigation-gdpr-index-index" after="-">
            <arguments>
				<argument name="path" xsi:type="string">gdpr/index</argument>
                <argument name="label" xsi:type="string">My Account Data</argument>
            </arguments>
        </block>
    </referenceBlock>
</body>
</page>
