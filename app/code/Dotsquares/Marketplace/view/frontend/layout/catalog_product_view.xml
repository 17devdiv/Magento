<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left"
	xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/
Framework/View/Layout/etc/page_configuration.xsd">
	<head>
		<css src="Dotsquares_Marketplace::css/style.css" />
	</head>
	<body>
		<referenceBlock name="product.info.type">
			<block class="Magento\Catalog\Block\Product\View"  before="-" name="product.info.sellerlink"
				template="Dotsquares_Marketplace::seller/linkseller.phtml" />
		</referenceBlock>
		<referenceContainer name="content">
			<container name="product.info.compareprice" htmlTag="div"
				htmlClass="product compareprice" before="product.info.details">
				<block class="Dotsquares\Marketplace\Block\Assignproduct\Manage"
					name="marketplace_assignproduct_compareprice" ifconfig="marketplace/general/enable_in_frontend"
					template="assignproduct/compareprice.phtml" />
			</container>

		</referenceContainer>
	</body>
</page>