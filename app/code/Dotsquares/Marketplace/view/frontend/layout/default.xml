<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left"
	xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
	<body>
		<referenceBlock name="header.links">
			<block class="Dotsquares\Marketplace\Block\Seller\Link" name="sellerlink"
				after="-" />
			<block class="Dotsquares\Marketplace\Block\Seller\Managesellers"
				name="manaage-sellers" after="-" />
		</referenceBlock>
		<referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Links"
                ifconfig="marketplace/general/enable_in_frontend" name="seller_account"
                before="customer_account_navigation" template="Dotsquares_Marketplace::account/navigation.phtml" cacheable="false">
            </block>
        </referenceBlock>
         <move element="seller_account" destination="sidebar.main" before="-"/>
	</body>
</page>