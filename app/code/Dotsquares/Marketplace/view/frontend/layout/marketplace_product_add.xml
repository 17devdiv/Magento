<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left"
	xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
	<head>
		<css src="Dotsquares_Marketplace::css/style.css" />
        	<script src="Dotsquares_Marketplace::js/configurable.js" />
           	<script src="Dotsquares_Marketplace::js/downloadable.js" />
    </head>
	<update handle="customer_account" />
	<body>
		<referenceContainer name="content">
			<block class="Dotsquares\Marketplace\Block\Product\Add" name="marketplace_product_add" template="product/form.phtml" before="-">
                 <block class="Dotsquares\Marketplace\Block\Product\Add" name="marketplace_product_customoptions_js" as="marketplace_product_customoptions_js" template="product/customoptions.phtml"  />
                 <block class="Dotsquares\Marketplace\Block\Product\Add" name="marketplace_product_downloadable" as="marketplace_product_downloadable" template="product/downloadable.phtml"  />
                 <block class="Dotsquares\Marketplace\Block\Product\Add" name="marketplace_product_downloadable_edit" as="marketplace_product_downloadable_edit" template="product/edit_downloadable.phtml"  />
                 <block class="Dotsquares\Marketplace\Block\Product\Add" name="marketplace_product_downloadable_additional" as="marketplace_product_downloadable_additional" template="product/downloadable_additional.phtml"  />
                 <block class="Dotsquares\Marketplace\Block\Product\Image" name="marketplace_product_video_add" as="marketplace_product_video_add" template="product/video.phtml"  />
            </block>
			<block class="Dotsquares\Marketplace\Block\Product\Configurable" name="marketplace_product_configurable" template="product/configurable.phtml" />
			<block class="Dotsquares\Marketplace\Block\Product\Add" name="marketplace_product_submit" template="product/submit.phtml" />
		</referenceContainer>
	</body>
</page>
