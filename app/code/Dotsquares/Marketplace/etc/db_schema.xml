<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
  <table name="marketplace_seller" resource="default" engine="innodb" comment="Seller Profile Table">
    <column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="ID"/>
    <column xsi:type="text" name="customer_id" nullable="false" comment="Customer Id"/>
    <column xsi:type="text" name="email" nullable="false" comment="Email"/>
    <column xsi:type="datetime" name="created_at" on_update="false" nullable="false" comment="Created At"/>
    <column xsi:type="smallint" name="status" padding="6" unsigned="false" nullable="false" identity="false" comment="Status"/>
    <column xsi:type="text" name="facebook_id" nullable="false" comment="Facebook Id"/>
    <column xsi:type="text" name="twitter_id" nullable="false" comment="Twitter Id"/>
    <column xsi:type="text" name="google_id" nullable="false" comment="Google Id"/>
    <column xsi:type="text" name="linked_id" nullable="false" comment="Linked Id"/>
    <column xsi:type="text" name="desc" nullable="false" comment="Description"/>
    <column xsi:type="text" name="paypal_id" nullable="false" comment="Paypal Id"/>
    <column xsi:type="text" name="bank_payment" nullable="false" comment="Bank Payment"/>
    <column xsi:type="text" name="store_name" nullable="false" comment="Store Name"/>
    <column xsi:type="text" name="contact" nullable="false" comment="Contact"/>
    <column xsi:type="text" name="commission" nullable="false" comment="Commission"/>
    <column xsi:type="text" name="country" nullable="false" comment="Country"/>
    <column xsi:type="text" name="state" nullable="false" comment="State"/>
    <column xsi:type="text" name="meta_keywords" nullable="false" comment="Meta Keywords"/>
    <column xsi:type="text" name="meta_description" nullable="false" comment="Meta Description"/>
    <column xsi:type="text" name="logo_name" nullable="false" comment="Logo Name"/>
    <column xsi:type="text" name="banner_name" nullable="false" comment="Banner Name"/>
    <column xsi:type="text" name="store_url" nullable="false" comment="Store Url"/>
    <column xsi:type="text" name="show_profile" nullable="false" comment="Show Profile"/>
    <column xsi:type="text" name="address" nullable="false" comment="Address"/>
    <column xsi:type="decimal" name="national_shipping_amount" scale="4" precision="12" unsigned="false" nullable="false" default="0" comment="National Shipping Amount"/>
    <column xsi:type="decimal" name="international_shipping_amount" scale="4" precision="12" unsigned="false" nullable="false" default="0" comment="International Shipping Amount"/>
    <column xsi:type="decimal" name="received_amount" scale="4" precision="12" unsigned="false" nullable="false" default="0" comment="Received Amount From Admin(Owner)"/>
    <column xsi:type="decimal" name="remaining_amount" scale="4" precision="12" unsigned="false" nullable="false" default="0" comment="Remaining Amount From Admin(Owner)"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="id"/>
    </constraint>
  </table>
  <table name="marketplace_sellerorder" resource="default" engine="innodb" comment="Seller Order Table">
    <column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="Id"/>
    <column xsi:type="int" name="order_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Order Id"/>
    <column xsi:type="int" name="seller_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Seller Id"/>
    <column xsi:type="decimal" name="commission" scale="4" precision="12" unsigned="false" nullable="true" comment="Commission"/>
    <column xsi:type="decimal" name="seller_product_total" scale="4" precision="12" unsigned="false" nullable="true" comment="Seller Product Total"/>
    <column xsi:type="decimal" name="seller_amount" scale="4" precision="12" unsigned="false" nullable="true" comment="Seller Amount"/>
    <column xsi:type="smallint" name="is_invoiced" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Invoiced"/>
    <column xsi:type="smallint" name="is_shipped" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Shipped"/>
    <column xsi:type="smallint" name="is_refunded" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Refunded"/>
    <column xsi:type="smallint" name="is_returned" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Returned"/>
    <column xsi:type="smallint" name="is_canceled" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Canceled"/>
    <column xsi:type="varchar" name="status" nullable="true" length="255" comment="Status"/>
    <column xsi:type="varchar" name="increment_id" nullable="true" length="255" comment="Increment Id"/>
    <column xsi:type="int" name="billing_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Billing Id"/>
    <column xsi:type="int" name="shipping_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Shipping Id"/>
    <column xsi:type="int" name="quote_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Quote Id"/>
    <column xsi:type="decimal" name="shipping_amount" scale="4" precision="12" unsigned="false" nullable="true" comment="Shipping Amount"/>
    <column xsi:type="varchar" name="order_currency_code" nullable="true" length="3" comment="Currency Code"/>
    <column xsi:type="int" name="customer_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Customer Id"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="id"/>
    </constraint>
  </table>
  <table name="marketplace_sellerorderitems" resource="default" engine="innodb" comment="Seller Order Items Table">
    <column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="Id"/>
    <column xsi:type="int" name="order_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Order Id"/>
    <column xsi:type="int" name="seller_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Seller Id"/>
    <column xsi:type="int" name="order_item_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Order Item Id"/>
    <column xsi:type="int" name="product_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Product Id"/>
    <column xsi:type="varchar" name="product_sku" nullable="true" length="255" comment="Product Sku"/>
    <column xsi:type="decimal" name="product_qty" scale="4" precision="12" unsigned="false" nullable="true" comment="Product Qty"/>
    <column xsi:type="varchar" name="product_name" nullable="true" length="255" comment="Product Name"/>
    <column xsi:type="text" name="options" nullable="true" comment="Options"/>
    <column xsi:type="smallint" name="is_canceled" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Canceled"/>
    <column xsi:type="varchar" name="status" nullable="true" length="255" comment="Status"/>
    <column xsi:type="int" name="parent_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Parent Id"/>
    <column xsi:type="int" name="quote_item_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Quote Item Id"/>
    <column xsi:type="int" name="quote_id" padding="10" unsigned="true" nullable="true" identity="false" comment="Quote Id"/>
    <column xsi:type="datetime" name="created_at" on_update="false" nullable="false" comment="Created At"/>
    <column xsi:type="decimal" name="qty_canceled" scale="4" precision="12" unsigned="false" nullable="true" comment="Qty Canceled"/>
    <column xsi:type="decimal" name="commission" scale="4" precision="12" unsigned="false" nullable="true" comment="Commission"/>
    <column xsi:type="decimal" name="product_price" scale="4" precision="12" unsigned="false" nullable="true" comment="Product Price"/>
    <column xsi:type="decimal" name="base_product_price" scale="4" precision="12" unsigned="false" nullable="true" comment="Base Product Price"/>
    <column xsi:type="smallint" name="is_buyer_canceled" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Buyer Canceled"/>
    <column xsi:type="smallint" name="is_buyer_refunded" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Buyer Refunded"/>
    <column xsi:type="smallint" name="is_buyer_returned" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Buyer Returned"/>
    <column xsi:type="smallint" name="is_refunded" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Refunded"/>
    <column xsi:type="smallint" name="is_returned" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Returned"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="id"/>
    </constraint>
  </table>
  <table name="marketplace_sellerreview" resource="default" engine="innodb" comment="Seller Review Table">
    <column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="ID"/>
    <column xsi:type="int" name="seller_id" padding="11" unsigned="false" nullable="false" identity="false" comment="Seller Id"/>
    <column xsi:type="int" name="customer_id" padding="11" unsigned="false" nullable="false" identity="false" comment="Customer Id"/>
    <column xsi:type="int" name="rating" padding="11" unsigned="false" nullable="false" identity="false" comment="Rating"/>
    <column xsi:type="varchar" name="review" nullable="true" length="255" comment="Review"/>
    <column xsi:type="int" name="store_id" padding="11" unsigned="false" nullable="false" identity="false" comment="Store Id"/>
    <column xsi:type="datetime" name="created_at" on_update="false" nullable="false" comment="Created At"/>
    <column xsi:type="smallint" name="status" padding="5" unsigned="true" nullable="true" identity="false" comment="Status"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="id"/>
    </constraint>
  </table>
  <table name="marketplace_subscription_plans" resource="default" engine="innodb" comment="Subscription Plans Table">
    <column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="ID"/>
    <column xsi:type="varchar" name="plan_name" nullable="true" length="255" comment="Plan Name"/>
    <column xsi:type="varchar" name="subscription_period_type" nullable="true" length="255" comment="Subscription Period Type"/>
    <column xsi:type="int" name="period_frequency" padding="11" unsigned="false" nullable="true" identity="false" comment="Period Frequency"/>
    <column xsi:type="varchar" name="max_product_count" nullable="true" length="255" comment="Max Product Count"/>
    <column xsi:type="decimal" name="fee" scale="4" precision="12" unsigned="false" nullable="true" comment="Fee"/>
    <column xsi:type="smallint" name="status" padding="5" unsigned="true" nullable="true" identity="false" comment="Status"/>
    <column xsi:type="datetime" name="created_at" on_update="false" nullable="false" comment="Created At"/>
    <column xsi:type="datetime" name="updated_at" on_update="false" nullable="false" comment="Updated At"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="id"/>
    </constraint>
  </table>
  <table name="marketplace_subscription_profiles" resource="default" engine="innodb" comment="Subscription Profiles Table">
    <column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="ID"/>
    <column xsi:type="int" name="plan_id" padding="11" unsigned="false" nullable="false" identity="false" comment="Plan Id"/>
    <column xsi:type="varchar" name="plan_name" nullable="true" length="255" comment="Plan Name"/>
    <column xsi:type="varchar" name="subscription_period_type" nullable="true" length="255" comment="Subscription Period Type"/>
    <column xsi:type="int" name="period_frequency" padding="11" unsigned="false" nullable="true" identity="false" comment="Period Frequency"/>
    <column xsi:type="varchar" name="max_product_count" nullable="true" length="255" comment="Max Product Count"/>
    <column xsi:type="decimal" name="fee" scale="4" precision="12" unsigned="false" nullable="true" comment="Fee"/>
    <column xsi:type="int" name="payment_id" padding="11" unsigned="false" nullable="false" identity="false" comment="Payment Id"/>
    <column xsi:type="int" name="seller_id" padding="11" unsigned="false" nullable="false" identity="false" comment="Seller Id"/>
    <column xsi:type="varchar" name="txn_id" nullable="true" length="255" comment="Transaction Id"/>
    <column xsi:type="varchar" name="invoice" nullable="true" length="255" comment="Invoice"/>
    <column xsi:type="datetime" name="started_at" on_update="false" nullable="false" comment="Started At"/>
    <column xsi:type="datetime" name="ended_at" on_update="false" nullable="false" comment="Ended At"/>
    <column xsi:type="datetime" name="created_at" on_update="false" nullable="false" comment="Created At"/>
    <column xsi:type="smallint" name="status" padding="5" unsigned="true" nullable="true" identity="false" comment="Status"/>
    <column xsi:type="varchar" name="receiver_email" nullable="true" length="255" comment="Receiver Email"/>
    <column xsi:type="varchar" name="payer_email" nullable="true" length="255" comment="Payer Email"/>
    <column xsi:type="varchar" name="payment_status" nullable="true" length="255" comment="Payment Status"/>
    <column xsi:type="varchar" name="item_name" nullable="true" length="255" comment="Item Name"/>
    <column xsi:type="varchar" name="base_currency_code" nullable="true" length="255" comment="Base Currency Code"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="id"/>
    </constraint>
  </table>
  <table name="marketplace_seller_payments" resource="default" engine="innodb" comment="Seller Payments Table">
    <column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="ID"/>
    <column xsi:type="decimal" name="paid_amount" scale="4" precision="12" unsigned="false" nullable="true" comment="Fee"/>
    <column xsi:type="int" name="seller_id" padding="11" unsigned="false" nullable="false" identity="false" comment="Seller Id"/>
    <column xsi:type="varchar" name="invoice" nullable="true" length="255" comment="Invoice"/>
    <column xsi:type="datetime" name="created_at" on_update="false" nullable="false" comment="Created At"/>
    <column xsi:type="datetime" name="ack_at" on_update="false" nullable="false" comment="Acknowledge At"/>
    <column xsi:type="smallint" name="is_ack" padding="5" unsigned="true" nullable="true" identity="false" comment="Is Acknowledged"/>
    <column xsi:type="varchar" name="comment" nullable="true" length="255" comment="Comment"/>
    <column xsi:type="varchar" name="payment_type" nullable="true" length="255" comment="Payment Type"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="id"/>
    </constraint>
  </table>
</schema>
