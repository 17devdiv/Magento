<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
        <css src="Dotsquares_Sitemap::css/sitemap.css" />
    </head>
     <body>
    

      <referenceBlock name="footer_links">
         <block class="Magento\Framework\View\Element\Html\Link\Current" ifconfig="seo/html_sitemap/id11" name="custom-footer-link">
             <arguments>
                 <argument name="label" xsi:type="string">Sitemap</argument>
                 <argument name="path" xsi:type="string">sitemap/</argument>
             </arguments>
         </block>
     </referenceBlock>
    

 
</body>
</page>